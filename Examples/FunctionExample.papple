Fnc Addition(Int X, Int Y) -> Int
    Rtn X + Y

Fnc Subtraction(Int X, Int Y) -> Int
    Rtn X - Y

Fnc Read_File(String FileName) -> Rtn
    String Contents = Open(File).Read_Lines()
    Out(Contents)

Fnc Is_Even(Int Number) -> Bln
    If Number % 2 == 0
        Rtn True
    Else
        Rtn False